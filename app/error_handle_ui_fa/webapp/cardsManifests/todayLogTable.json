{
  "sap.app": {
    "id": "Id_Recent_Table",
    "type": "card"
  },
  "sap.card": {
    "type": "Table",
    "header": {
      "title": "Recent Error records"
    },
    "content": {
      "data": {
        "request": {
          "url": "/odata/v4/catalog/ErrorLogSet?$top=10&$orderby=createdAt desc",
          "withCredentials": true
        },
        "path": "/value"
      },
      "row": {
        "columns": [
          {
            "title": "IFlow Name",
            "value": "{iFlow_name}"
          },
          {
            "title": "Date",
            "value": "{createdAt}"
          },
          {
            "title": "Status",
            "value": "{Status}"
          },
          {
            "title": "Receiver System",
            "value": "{Receiver_System}"
          }
        ],
        "actions": [
          {
            "type": "Navigation",
            "url": "#ErrorLogSet-manage?filter=ID eq {ID}"
          }
        ]
      },
      "maxItems": 9,
      "showNoData": true,
      "scrollable": true
    }
  }
}