    "TotalErrors": {
        "sap.app": {
            "id": "Id_Total_Errors",
            "type": "card"
        },
        "sap.card": {
            "type": "Object",
            "header": {
                "title": "Total Errors",
                "subTitle": "Recorded error count",
                "icon": {
                    "src": "sap-icon://sum"
                }
            },
            "configuration": {
                "destinations": {
                    "srv": {
                        "name": "srv-api",
                        "defaultUrl": "/b4b3b066-86fd-47cc-8c19-db49d3580052.POC_EH_managed.comapperrorhandleuifa-0.0.1/odata/v4/catalog/"
                    }
                }
            },
            "actions": [
                {
                    "type": "Navigation",
                    "url": ""
                }
            ],
            "content": {
                "data": {
                    "request": {
                        "url": "{{destinations.srv}}/CatalogService.countErrors()",
                        "method": "GET"
                    },
                    "path": "/value/0"
                },
                "groups": [
                    {
                        "title": "Error Count",
                        "items": [
                            {
                                "label": "Total Errors",
                                "value": "{Value}"
                            }
                        ]
                    }
                ]
            }
        }
    },
