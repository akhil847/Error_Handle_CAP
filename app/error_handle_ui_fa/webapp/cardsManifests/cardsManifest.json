    "RecentTable": {
        "sap.app": {
            "id": "Id_Recent_Table",
            "type": "card"
        },
        "sap.card": {
            "type": "Table",
            "header": {
                "title": "Recent Error records"
            },
            "configuration": {
                "destinations": {
                    "srv": {
                        "name": "srv-api",
                        "defaultUrl": "/b4b3b066-86fd-47cc-8c19-db49d3580052.POC_EH_managed.comapperrorhandleuifa-0.0.1/odata/v4/catalog/"
                    }
                }
            },
            "content": {
                "data": {
                    "request": {
                        "url": "{{destinations.srv}}/ErrorLogSet?$top=10&$orderby=createdAt desc"
                    },
                    "path": "/value"
                },
                "row": {
                    "columns": [
                        {
                            "title": "IFlow Name",
                            "value": "{iFlow_name}"
                        },
                        {
                            "title": "Date",
                            "value": "{createdAt}"
                        },
                        {
                            "title": "Status",
                            "value": "{Status}"
                        },
                        {
                            "title": "Receiver System",
                            "value": "{Receiver_System}"
                        }
                    ],
                    "actions": [
                        {
                            "type": "Navigation",
                            "url": "#ErrorLogSet-manage?filter=ID eq {ID}"
                        }
                    ]
                },
                "maxItems": 9,
                "showNoData": true,
                "scrollable": true
            }
        }
    }
